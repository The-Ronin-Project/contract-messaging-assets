        },
        "contentType": {
          "type": "string"
        },
        "url": {
          "type": "string"
        },
        "hash": {
          "type": "string"
        },
        "createdAt": {
          "type": "string"
        },
        "updatedAt": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "contentType",
        "resourceAttribute",
        "url",
        "hash",
        "createdAt",
        "updatedAt"
      ]
    }
  }
