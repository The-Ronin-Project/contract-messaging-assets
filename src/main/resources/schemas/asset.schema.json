{
  "$id": "stored-assets-v1",
  "title": "Asset Stored Event",
  "description": "A list of stored Assets",
  "type": "object",
  "properties": {
    "tenantId": {
      "type": "string"
    },
    "patientId": {
      "type": "string"
    },
    "resourceType": {
      "type": "string"
    },
    "resourceId": {
      "type": "string"
    },
    "assets": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/Asset"
      }
    },
    "hash": {
      "type": "string"
