{
  "$id": "asset",
  "title": "Asset Stored Event",
  "description": "A list of stored Assets",
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "tenantId": {
      "type": "string"
    },
    "patientId": {
      "type": "string"
    },
    "attachments": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/Attachment"
      }
    },
    "hash": {
      "type": "string"
    },
    "resourceType": {
      "$ref": "#/$defs/ResourceType"
    },
    "resourceId": {
      "type": "string"
    },
    "resourceAttribute": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },
    "updatedAt": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "tenantId",
    "patientId",
    "attachments",
    "resourceId",
    "resourceType",
    "resourceAttribute",
    "createdAt",
    "updatedAt"
  ],
  "$defs": {
    "ResourceType": {
      "type": "string",
      "enum": [
        "DiagnosticReport",
        "DocumentReference",
        "Patient"
      ]
    },
    "Attachment": {
      "type": "object",
      "properties": {
        "contentType": {
          "type": "string"
        },
        "url": {
          "type": "string"
        },
        "hash": {
          "type": "string"
        }
      },
      "required": [
        "contentType",
        "url"
      ]
    }
  }
}
